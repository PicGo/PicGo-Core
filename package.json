{
  "name": "picgo",
  "version": "1.4.26",
  "description": "A tool for picture uploading",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "picgo": "./bin/picgo"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "node ./bin/picgo",
    "lint": "eslint src/**/*.ts && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node --experimental-json-modules esbuild.mjs",
    "dev": "node --experimental-json-modules esbuild.mjs --watch",
    "patch": "npm version patch && git push origin master && git push origin --tags",
    "minor": "npm version minor && git push origin master && git push origin --tags",
    "major": "npm version major && git push origin master && git push origin --tags",
    "cz": "git-cz",
    "release": "bump-version"
  },
  "keywords": [
    "picture",
    "upload",
    "util"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./node_modules/@picgo/bump-version/.cz-config.js"
    }
  },
  "commitlint": {
    "extends": [
      "./node_modules/@picgo/bump-version/commitlint-picgo"
    ]
  },
  "author": "Molunerfinn",
  "license": "MIT",
  "devDependencies": {
    "@picgo/bump-version": "^1.1.2",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/cross-spawn": "^6.0.0",
    "@types/ejs": "^3.0.5",
    "@types/fs-extra": "^5.0.4",
    "@types/globby": "^9.1.0",
    "@types/image-size": "^0.0.29",
    "@types/inquirer": "^0.0.42",
    "@types/lodash-es": "^4.17.5",
    "@types/lowdb": "^1.0.4",
    "@types/md5": "^2.1.32",
    "@types/mime-types": "^2.1.0",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.5.2",
    "@types/request-promise-native": "^1.0.15",
    "@types/resolve": "^0.0.8",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "3",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "conventional-changelog": "^3.0.6",
    "copyfiles": "^2.1.0",
    "cz-customizable": "^5.10.0",
    "esbuild": "^0.13.3",
    "esbuild-plugin-inline-import": "^1.0.1",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "execa": "^5.1.1",
    "husky": "^1.3.1",
    "pre-commit": "^1.2.2",
    "rollup": "^2.58.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^8.1.0",
    "comment-json": "^2.3.1",
    "cross-spawn": "^6.0.5",
    "dayjs": "^1.7.4",
    "download-git-repo": "^3.0.2",
    "ejs": "^2.6.1",
    "fs-extra": "^6.0.1",
    "globby": "^11.0.4",
    "image-size": "^0.8.3",
    "inquirer": "^6.0.0",
    "is-wsl": "^2.2.0",
    "lodash-es": "^4.17.21",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "md5": "^2.2.1",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "qiniu": "^7.2.1",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "resolve": "^1.8.1",
    "rimraf": "^2.6.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PicGo/PicGo-Core.git"
  },
  "picBed": {
    "current": "smms"
  },
  "plugins": {},
  "engines": {
    "node": ">= 12.0.0"
  }
}
